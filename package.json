{
  "name": "animalfinder",
  "version": "1.0.0",
  "description": "ONG Animal Finder é especializada em ajudar a encontrar animais perdidos",
  "main": "index.js",
  "author": "André Luiz Hashiguchi",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "dev": "./node_modules/.bin/nodemon index.js",
    "eslint": "./node_modules/.bin/eslint . --ext .js",
    "test": "NODE_ENV=test ./node_modules/.bin/lab -l -v -m 60000 -r console -o stdout -r lcov -o coverage/lcov.info --pattern 'spec' ./src/test/index.spec.js ./src/modules/$1",
    "test-report": "NODE_ENV=test ./node_modules/.bin/lab -l -v -m 60000 -r console --coverage -o stdout -r html -o coverage/coverage.html -r lcov -o coverage/lcov.info --pattern 'spec' src/test/index.spec.js src/modules/$1",    
    "migration:up": "./node_modules/.bin/sequelize db:migrate",
    "migration:down": "./node_modules/.bin/sequelize db:migrate:undo:all",
    "seeders:up": "./node_modules/.bin/sequelize db:seed:all",
    "seeders:down": "./node_modules/.bin/sequelize db:seed:undo:all"
  },
  "dependencies": {
    "@hapi/boom": "^7.3.0",
    "@hapi/catbox": "^10.0.6",
    "@hapi/catbox-memory": "^4.0.1",
    "@hapi/good": "^8.1.2",
    "@hapi/good-console": "^8.0.0",
    "@hapi/good-squeeze": "^5.1.0",
    "@hapi/hapi": "^18.1.0",
    "@hapi/joi": "^14.3.1",
    "aws-sdk": "^2.553.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.1",
    "code": "^5.2.4",
    "disinfect": "^1.0.0",
    "dotenv": "^8.2.0",    
    "good-http": "^6.1.3",
    "hapi-auth-jwt2": "^8.7.0",
    "hapi-boom-decorators": "^4.1.2",
    "hapi-swagger": "^10.2.0",
    "imagemagick": "^0.1.3",
    "inert": "^5.1.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "path": "^0.12.7",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "rate-limiter-flexible": "^1.1.6",
    "redis": "^2.8.0",
    "sequelize": "^5.15.1",
    "sequelize-cli": "^4.1.1",    
    "uuid": "^3.3.3",
    "vision": "^5.4.4",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.27",
    "@types/boom": "^7.3.0",
    "@types/code": "^4.0.5",
    "@types/hapi": "^18.0.2",
    "@types/inert": "^5.1.2",
    "@types/joi": "^14.3.3",
    "@types/lab": "^11.1.0",
    "@types/node": "^12.7.12",
    "@types/vision": "^5.3.6",
    "code": "^5.2.4",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "lab": "^18.0.2",
    "nodemon": "^1.19.3"
  }
}
